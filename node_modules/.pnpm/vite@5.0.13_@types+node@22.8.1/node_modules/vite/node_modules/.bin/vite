#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/vite@5.0.13_@types+node@22.8.1/node_modules/vite/bin/node_modules:/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/vite@5.0.13_@types+node@22.8.1/node_modules/vite/node_modules:/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/vite@5.0.13_@types+node@22.8.1/node_modules:/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/vite@5.0.13_@types+node@22.8.1/node_modules/vite/bin/node_modules:/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/vite@5.0.13_@types+node@22.8.1/node_modules/vite/node_modules:/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/vite@5.0.13_@types+node@22.8.1/node_modules:/home/wcy/roundraichu.github.io/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
